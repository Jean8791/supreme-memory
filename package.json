{
  "name": "supreme-memory",
  "version": "1.0.0",
  "description": "Repositório criado para organizar meus estudos de desenvolvimento. Aqui estou praticando conceitos fundamentais, explorando novas tecnologias e criando pequenos projetos enquanto avanço no meu aprendizado contínuo.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run service:up && next dev",
    "service:up": "docker compose -f infra/docker-compose.yaml up -d",
    "service:stop": "docker compose -f infra/docker-compose.yaml stop",
    "service:down": "docker compose -f infra/docker-compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --runInBand",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jean8791/supreme-memory.git"
  },
  "author": "Jean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jean8791/supreme-memory/issues"
  },
  "homepage": "https://github.com/Jean8791/supreme-memory#readme",
  "dependencies": {
    "dotenv": "^16.4.4",
    "dotenv-expand": "^11.0.6",
    "next": "16.0.7",
    "node-pg-migrate": "8.0.4",
    "pg": "8.11.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.7.4"
  }
}
